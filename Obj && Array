const bicicleta = {
  modelo: "Speed",
  tamanho: 56,
  pneus: 700
};

const marcas = ["Riva", "Chandan", "RepÃºblica"];

function listaPropriedades(objeto) {
  let propriedades = [];
  for (let propriedade in objeto) propriedades.push(propriedade);
  return propriedades;
}

let listaElementos = (array) => {
  let elementos = [];
  for (let elemento of array) elementos.push(elemento);
  return elementos;
};

console.log(bicicleta);
console.log(marcas);
